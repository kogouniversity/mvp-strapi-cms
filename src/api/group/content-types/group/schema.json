        },
        "users": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "plugin::users-permissions.user",
            "mappedBy": "groups"
        },
        "address": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "api::address.address"
        },
        "hasAddress": {
            "type": "boolean",
            "default": false
        },
        "regionRestricted": {
            "type": "boolean",
            "default": false
        },
        "enabled": {
            "type": "boolean"
        }
    }
