{
  "kind": "collectionType",
  "collectionName": "groups",
  "info": {
    "singularName": "group",
    "pluralName": "groups",
    "displayName": "Group",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "minLength": 3,
      "unique": true
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "group"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "groups"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "hasAddress": {
      "type": "boolean",
      "default": false
    },
    "regionRestricted": {
      "type": "boolean",
      "default": false
    },
    "enabled": {
      "type": "boolean",
      "default": true
    },
    "description": {
      "type": "text",
      "maxLength": 100
    },
    "userCount": {
      "type": "integer",
      "default": 0
    },
    "imageProfile": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::image-profile.image-profile",
      "inversedBy": "group"
    },
    "isSchool": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
