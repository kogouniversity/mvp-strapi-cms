{
  "kind": "collectionType",
  "collectionName": "image_profiles",
  "info": {
    "singularName": "image-profile",
    "pluralName": "image-profiles",
    "displayName": "ImageProfile"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "profileName": {
      "type": "string"
    },
    "src": {
      "type": "string"
    },
    "width": {
      "type": "string",
      "required": true
    },
    "height": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::image.image",
      "inversedBy": "profiles"
    },
    "group": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::group.group",
      "mappedBy": "image_profile"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "imageProfile"
    }
  }
}
